<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ISEN.MSH.Nhibernate.Models" namespace="ISEN.MSH.Nhibernate.Models.WorkFlow">

	<class name="BaseFlowRequirement" table="T_BaseFlowRequirement">
		<cache usage="read-write"/>
		<id name="ID" type="Guid">
			<column name="BaseFlowRequirementID" index="PK_BaseFlowRequirement"/>
			<generator class="assigned" />
		</id>

		<!--相关数据名称-->
		<property name="Name" type="string">
			<column name="Name" not-null="true"/>
		</property>

		<!--相关数据值-->
		<!--<property name="Value" type="string">
			<column name="Value" not-null="true"/>
		</property>-->

		<!--流程标识-->
		<many-to-one name="BaseFlow" column="BaseFlowID" class="BaseFlow" not-null="true"
                       foreign-key="FK_BaseFlowRequirement_BaseFlow"/>

		<!--建立日期-->
		<property name="CreateDate" type="DateTime">
			<column name="CreateDate" not-null="true"/>
		</property>

	</class>

</hibernate-mapping>