<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
           assembly="ISEN.MSH.Nhibernate.Models">

	<class name="ISEN.MSH.Nhibernate.Models.WorkFlow.FlowVertexInstances" table="T_FlowVertexInstances">
		<cache usage="read-write"/>
		<id name="ID" type="Guid">
			<column name="FlowVertexInstancesID" index="PK_FlowVertexInstances"/>
			<generator class="assigned" />
		</id>
		
		<!--流程标识-->
		<many-to-one name="BaseFlowVertex" column="BaseFlowVertexID" class="ISEN.MSH.Nhibernate.Models.WorkFlow.BaseFlowVertex" not-null="true"
                       foreign-key="FK_FlowVertexInstances_BaseFlowVertex"/>
	
		<!--当前工作项状态-->
		<property name="State" type="string">
			<column name="State" not-null="true"/>
		</property>

		<!--参与者-->
		<many-to-one name="Participator" column="ParticipatorID" class="ISEN.MSH.Nhibernate.Models.UserInfo" not-null="true"
                       foreign-key="FK_FlowVertexInstances_UserInfo"/>

		<!--建立日期-->
		<property name="CreateDate" type="DateTime">
			<column name="CreateDate" not-null="true"/>
		</property>

	</class>

</hibernate-mapping>