<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		   assembly="ISEN.MSH.Nhibernate.Models"
		   namespace="ISEN.MSH.Nhibernate.Models.WorkFlow">

	<class name="FlowRequirementInstances" table="T_FlowRequirementInstances">
		<cache usage="read-write"/>
		<id name="ID" type="Guid">
			<column name="FlowRequirementInstancesID" index="PK_FlowRequirementInstances"/>
			<generator class="assigned" />
		</id>

		<!--建立日期-->
		<property name="CreateDate" type="DateTime">
			<column name="CreateDate" not-null="true"/>
		</property>

		<!--流程分支条件-->
		<property name="Name" type="string">
			<column name="Name" not-null="true"/>
		</property>

		<!--流程标识（记录当前流程属于哪个流程）-->
		<many-to-one name="FlowInstances" column="FlowInstancesID" class="FlowInstances" not-null="true"
                      cascade="save-update" foreign-key="FK_FlowRequirementInstances_FlowInstances"/>

	</class>

</hibernate-mapping>