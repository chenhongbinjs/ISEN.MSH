<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		   assembly="ISEN.MSH.Nhibernate.Models">

	<class name="ISEN.MSH.Nhibernate.Models.WorkFlow.FlowInstances" table="T_FlowInstances">
		<cache usage="read-write"/>
		<id name="ID" type="Guid">
			<column name="FlowInstancesID" index="PK_FlowInstances"/>
			<generator class="assigned" />
		</id>

		<!--建立日期-->
		<property name="CreateDate" type="DateTime">
			<column name="CreateDate" not-null="true"/>
		</property>

		<!--流程分支条件-->
		<property name="State" type="string">
			<column name="State" length="50" not-null="true"/>
		</property>

		<!--流程标识（记录当前流程属于哪个流程）-->
		<many-to-one name="BaseFlow" column="BaseFlowID" class="ISEN.MSH.Nhibernate.Models.WorkFlow.BaseFlow" not-null="true"
                      cascade="save-update" foreign-key="FK_FlowInstances_BaseFlow"/>
		<!--流程创建者-->
		<many-to-one name="Creator" column="CreatorID" class="ISEN.MSH.Nhibernate.Models.UserInfo" not-null="true"
              cascade="save-update" foreign-key="FK_FlowInstances_UserInfo"/>
		<!--流程分值条件列表-->
		<bag name="FlowRequirementInstancesList" generic="true" inverse="true" cascade="all-delete-orphan" table="T_FlowRequirementInstances">
			<key column="FlowRequirementInstancesID" foreign-key="FK_FlowRequirementInstances_FlowInstances"/>
			<one-to-many class="ISEN.MSH.Nhibernate.Models.WorkFlow.FlowRequirementInstances"/>
		</bag>

	</class>

</hibernate-mapping>